{
  "name": "@kizo/shared",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "src",
    "generated"
  ],
  "scripts": {
    "gen:validate": "redocly lint openapi.yaml",
    "gen:zod": "mkdir -p generated && openapi-zod-client openapi.yaml -o generated/api.ts --export-schemas --export-types --with-docs --with-description",
    "gen:all": "pnpm run gen:validate && pnpm run gen:zod"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@redocly/cli": "^2.12.0",
    "@repo/typescript-config": "workspace:*",
    "openapi-zod-client": "^1.18.3",
    "prettier": "^3.0.0",
    "typescript": "latest"
  }
}
