{
  "name": "kizo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:all": "dotenv -e .env -- turbo dev",
    "build:all": "dotenv -e .env -- turbo build",
    "lint:all": "turbo lint",
    "format:all": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo check-types",
    "prepare:backend": "npm run prisma:generate",
    "prisma:generate": "dotenv -e .env -- npm -w @kizo/db exec prisma generate",
    "prisma:migrate": "dotenv -e .env -- npm -w @kizo/db exec prisma migrate dev",
    "prisma:push": "dotenv -e .env -- npm -w @kizo/db exec prisma db push",
    "prisma:reset": "dotenv -e .env -- npm -w @kizo/db exec prisma migrate reset",
    "prisma:seed": "dotenv -e .env -- tsx packages/kizo-db/scripts/seed-stress.ts",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@vitest/coverage-v8": "^4.0.15",
    "@types/express": "^5.0.3",
    "@types/json2csv": "^5.0.7",
    "@types/multer": "^2.0.0",
    "@types/node": "25.1.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/pg": "^8.15.6",
    "openapi-zod-client": "^1.18.3",
    "prisma": "7.2.0",
    "supertest": "^7.1.4",
    "turbo": "^2.6.1",
    "typescript": "5.9.2",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "7.2.0",
    "@supabase/supabase-js": "^2.88.0",
    "argon2": "^0.43.0",
    "bullmq": "^5.67.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv-cli": "^11.0.0",
    "express": "^5.1.0",
    "json2csv": "6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "pino": "^10.3.0",
    "pino-loki": "^3.0.0",
    "prom-client": "^15.1.3",
    "redis": "^5.10.0",
    "sharp": "^0.34.5",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "yaml": "^2.8.2",
    "zod": "^3"
  },
  "packageManager": "npm@11.0.0",
  "engines": {
    "node": ">=18"
  }
}
