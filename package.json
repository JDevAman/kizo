{
  "name": "kizo",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env -- turbo run dev",
    "build": "dotenv -e .env -- turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "prepare:backend": "pnpm run prisma:generate",
    "prisma:generate": "dotenv -e .env -- pnpm --filter @kizo/db exec prisma generate",
    "prisma:migrate": "dotenv -e .env -- pnpm --filter @kizo/db exec prisma migrate dev",
    "prisma:push": "dotenv -e .env -- pnpm --filter @kizo/db exec prisma db push",
    "prisma:reset": "dotenv -e .env -- pnpm --filter @kizo/db exec prisma migrate reset"
  },
  "devDependencies": {
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.15",
    "supertest": "^7.1.4",
    "turbo": "^2.6.1",
    "typescript": "5.9.2",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@zodios/core": "^10.9.6",
    "dotenv-cli": "^11.0.0",
    "zod": "^3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "yargs-parser": "21.1.1",
      "magicast": "^0.3.5"
    }
  }
}
